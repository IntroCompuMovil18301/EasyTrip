<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="javeriana.edu.co.easytrip">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="EasyTrip"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        >
        <activity android:name=".PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistroPerfilActivity" />
        <activity android:name=".SubirFotoActivity" />
        <activity android:name=".RegistroHuespedActivity" />
        <activity android:name=".BuscarAlojamientoActivity" />
        <activity android:name=".PerfilHuespedActivity" />
        <activity android:name=".HomeHuespedActivity"/>


        <activity android:name=".VerAlojamientoActivity" />
        <activity android:name=".CalendarioReservarActivity" />
        <activity android:name=".VerMashActivity" />
        <activity android:name=".HomeAnfitrionActivity" />
        <activity android:name=".VerReservaActivity" />
        <activity android:name=".AddAlojamientoActivity" />
        <activity android:name=".ActivityCompaneros" />
        <activity android:name=".ActivityMatshes" />
        <activity android:name=".VerHuespedActivity" />
        <activity android:name=".EditarPerfilHuespedActivity" />
        <activity android:name=".EditarPerfilAnfitrionActivity" />
        <activity android:name=".PerfilAnfitrionActivity"></activity>
    </application>

</manifest>
